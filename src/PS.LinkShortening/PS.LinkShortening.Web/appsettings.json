{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "Server=(local);Database=LinkShorteningDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },

  "Url": {
    "OverrideUrl": null,
    "KeyLength": 6,
    "_hint": "Excluding chars that can be confused with others such as 1, I, l and 0, O, o",
    "KeyChars": "23456789abcdefghjkmnpqrstuvwxyzABCDEFGHJKLMNPQRSTUVWXYZ"
  },

  "Security": {
    "AuthenticationTokens": [ "ValidToken123" ],
    "RequireTokenForCreate": true,
    "RequireTokenForGet": true
  },

  "Cache": {
    "ItemLimit": 10000,
    "NegativeCacheTimeoutSeconds": 60
  },

  "Google": {
    "AnalyticsId": "",
    "RecaptchaV3SiteKey": "",
    "RecaptchaV3SecretKey": ""
  }
}
